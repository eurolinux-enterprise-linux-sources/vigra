curdir=$(vigra_testdir)/voxelneighborhood

test: test.o $(PREINSTALL_IMPEX_LIB)
	$(CXX) -o test test.o $(PREINSTALL_IMPEX_LIB) $(IMPEX_DEPLIBS)
	./test || { rm ./test$(EXEEXT) ; exit 1; }

voxelneighborhood.exe: test.d
	devenv "..\test.sln" /build Release /project voxelneighborhood
	./voxelneighborhood.exe || { rm ./voxelneighborhood.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
