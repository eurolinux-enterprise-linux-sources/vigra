curdir=$(vigra_testdir)/multidistance

test: test.o $(PREINSTALL_IMPEX_LIB)
	$(CXX) -o test test.o $(PREINSTALL_IMPEX_LIB) $(IMPEX_DEPLIBS)
	./test || { rm ./test$(EXEEXT) ; exit 1; }

multidistance.exe: test.d
	devenv "..\test.sln" /build Release /project multidistance
	./multidistance.exe || { rm ./multidistance.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
