curdir=$(vigra_testdir)/volumelabeling

test: test.o $(PREINSTALL_IMPEX_LIB)
	$(CXX) -o test test.o $(PREINSTALL_IMPEX_LIB) $(IMPEX_DEPLIBS)
	./test || { rm ./test$(EXEEXT) ; exit 1; }

volumelabeling.exe: test.d
	devenv "..\test.sln" /build Release /project volumelabeling
	./volumelabeling.exe || { rm ./volumelabeling.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
