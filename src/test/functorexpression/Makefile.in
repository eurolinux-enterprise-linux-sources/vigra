curdir=$(vigra_testdir)/functorexpression

test: test.o
	$(CXX) -o test test.o
	./test || { rm ./test$(EXEEXT) ; exit 1; }

functorexpression.exe: test.d
	devenv "..\test.sln" /build Release /project functorexpression
	./functorexpression.exe || { rm ./functorexpression.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
