curdir=$(vigra_testdir)/multimorphology

test: test.o $(PREINSTALL_IMPEX_LIB)
	$(CXX) -o test test.o $(PREINSTALL_IMPEX_LIB) $(IMPEX_DEPLIBS)
	./test || { rm ./test$(EXEEXT) ; exit 1; }

multimorphology.exe: test.d
	devenv "..\test.sln" /build Release /project multimorphology
	./multimorphology.exe || { rm ./multimorphology.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
