curdir=$(vigra_testdir)/seededRegionGrowing3d

test: test.o $(PREINSTALL_IMPEX_LIB)
	$(CXX) -o test test.o $(PREINSTALL_IMPEX_LIB) $(IMPEX_DEPLIBS)
	./test || { rm ./test$(EXEEXT) ; exit 1; }

seededRegionGrowing3d.exe: test.d
	devenv "..\test.sln" /build Release /project seededRegionGrowing3d
	./seededRegionGrowing3d.exe || { rm ./seededRegionGrowing3d.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
