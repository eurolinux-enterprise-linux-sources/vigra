curdir=$(vigra_testdir)/watersheds3d

test: test.o $(PREINSTALL_IMPEX_LIB)
	$(CXX) -o test test.o $(PREINSTALL_IMPEX_LIB) $(IMPEX_DEPLIBS)
	./test || { rm ./test$(EXEEXT) ; exit 1; }

watersheds3d.exe: test.d
	devenv "..\test.sln" /build Release /project watersheds3d
	./watersheds3d.exe || { rm ./watersheds3d.exe ; exit 1; }

ifneq "$(MAKECMDGOALS)" "clean"
include test.d
endif

include ../Makefile.include
